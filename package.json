{
  "name": "ngx-svg",
  "version": "1.1.0",
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "whitelistedNonPeerDependencies": [
      "@svgdotjs/svg.js"
    ],
    "lib": {
      "entryFile": "ngx_svg.ts"
    }
  },
  "description": "Angular Library for creating SVG elements.",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "test": "ng test --watch=false --no-progress --browsers=ChromeHeadlessNoSandbox",
    "test:local": "ng test --watch=true --code-coverage",
    "ng:update": "ng update @angular/core @angular/cli --force",
    "lint": "ng lint",
    "packagr": "ng-packagr -p package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vvaldersteins/ngx-svg.git"
  },
  "keywords": [
    "SVG",
    "Angular",
    "ngx-svg",
    "ng"
  ],
  "author": "Valters Valdersteins",
  "bugs": {
    "url": "https://github.com/vvaldersteins/ngx-svg/issues"
  },
  "homepage": "https://github.com/vvaldersteins/ngx-svg#readme",
  "peerDependencies": {},
  "dependencies": {
    "@svgdotjs/svg.js": "^3.0.16"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular/animations": "^10.2.4",
    "@angular/cli": "^9.1.0",
    "@angular/common": "^10.2.4",
    "@angular/compiler": "^10.2.4",
    "@angular/compiler-cli": "^10.2.4",
    "@angular/core": "^10.2.4",
    "@angular/forms": "^10.2.4",
    "@angular/http": "^7.2.16",
    "@angular/language-service": "^10.2.4",
    "@angular/platform-browser": "^10.2.4",
    "@angular/platform-browser-dynamic": "^10.2.4",
    "@angular/router": "^10.2.4",
    "@types/jasmine": "^3.5.10",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^13.11.0",
    "acorn": "^7.1.1",
    "codecov": "^3.6.5",
    "codelyzer": "^5.2.2",
    "core-js": "^3.6.4",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "^3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "ng-packagr": "^9.1.0",
    "rxjs": "^6.5.5",
    "rxjs-compat": "^6.5.5",
    "ts-node": "^8.8.1",
    "tsickle": "^0.38.1",
    "tslib": "^1.11.1",
    "tslint": "^6.1.1",
    "typescript": "^4.0.5",
    "zone.js": "^0.10.3"
  }
}
